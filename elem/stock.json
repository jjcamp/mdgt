{
  "search": {
    "uriRoot":"https://www.google.com/finance",
    "searchPath":"/?q=*"
  },
  "root":{
    "xpath":"//*[@itemscope]",
    "items":[
      {
        "xpath":"//*[@itemprop='name']",
        "value":{
          "name":"Name",
          "type":"attr",
          "attr":"content"
        }
      },
      {
        "xpath":"//*[@itemprop='tickerSymbol']",
        "value":{
          "name":"Symbol",
          "type":"attr",
          "attr":"content"
        }
      },
      {
        "xpath":"//*[@itemprop='exchange']",
        "value":{
          "name":"Exchange",
          "type":"attr",
          "attr":"content"
        }
      },
      {
        "xpath":"//*[@itemprop='price']",
        "value":{
          "name":"Price",
          "type":"attr",
          "attr":"content"
        }
      },
      {
        "xpath":"//*[@itemprop='priceChange']",
        "value":{
          "name":"Change",
          "type":"attr",
          "attr":"content"
        }
      },
      {
        "xpath":"//*[@itemprop='priceChangePercent']",
        "value":{
          "name":"% Change",
          "type":"attr",
          "attr":"content"
        }
      }, 
      {
        "xpath":"//*[@itemprop='priceCurrency']",
        "value":{
          "name":"Currency",
          "type":"attr",
          "attr":"content"
        }
      }
    ]
  }
}
